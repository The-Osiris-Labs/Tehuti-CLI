{
  "name": "tehuti-cli",
  "version": "0.1.0",
  "description": "Tehuti CLI - Scribe of Code Transformations. An AI-powered coding assistant connecting to OpenRouter.",
  "type": "module",
  "bin": {
    "tehuti": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "AGENTS.md"
  ],
  "scripts": {
    "start": "tsx src/index.ts",
    "start:opentui": "echo 'OpenTUI requires Bun runtime. Run: bun src/tui-opentui.tsx' && exit 1",
    "start:ink": "tsx src/index.ts",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "biome check src/",
    "test": "vitest run",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ai",
    "cli",
    "coding-assistant",
    "openrouter",
    "mcp",
    "agent",
    "code-generation",
    "tehuti"
  ],
  "author": "Osiris Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/The-Osiris-Labs/Tehuti-CLI"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@ai-sdk/react": "^3.0.99",
    "@aiter/core": "^0.1.10",
    "@inquirer/prompts": "^8.2.1",
    "@lvce-editor/ripgrep": "^4.1.0",
    "@modelcontextprotocol/sdk": "^1.27.0",
    "@openrouter/ai-sdk-provider": "^2.2.3",
    "@opentui/core": "^0.1.80",
    "@opentui/react": "^0.1.80",
    "@types/turndown": "^5.0.6",
    "ai": "^6.0.97",
    "chalk": "^5.4.1",
    "chokidar": "^5.0.0",
    "clean-stack": "^5.2.0",
    "cli-spinners": "^3.4.0",
    "cli-truncate": "^5.1.1",
    "commander": "^14.0.0",
    "conf": "^15.1.0",
    "consola": "^3.4.0",
    "cosmiconfig": "^9.0.0",
    "diff": "^8.0.1",
    "dotenv": "^16.4.7",
    "exa-js": "^2.4.0",
    "figlet": "^1.10.0",
    "file-type": "^21.3.0",
    "fs-extra": "^11.3.0",
    "fuse.js": "^7.1.0",
    "gradient-string": "^3.0.0",
    "handle-cli-error": "^4.0.0",
    "ink": "^6.7.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "is-ci": "^4.1.0",
    "is-interactive": "^2.0.0",
    "is-unicode-supported": "^2.1.0",
    "just-bash": "^1.0.0",
    "listr2": "^10.1.0",
    "marked-terminal": "^7.3.0",
    "ora": "^9.3.0",
    "pdf-parse": "^2.4.5",
    "picocolors": "^1.1.0",
    "react": "^19.0.0",
    "serialize-error": "^12.0.0",
    "sharp": "^0.34.5",
    "shiki": "^3.2.1",
    "string-width": "^8.2.0",
    "supports-color": "^10.2.2",
    "supports-hyperlinks": "^4.4.0",
    "terminal-link": "^5.0.0",
    "terminal-size": "^4.0.0",
    "tinyglobby": "^0.2.14",
    "turndown": "^7.2.2",
    "undici": "^7.22.0",
    "update-notifier": "^7.3.1",
    "uuid": "^13.0.0",
    "wrap-ansi": "^10.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.0",
    "@types/update-notifier": "^6.0.8",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "vitest": "^3.0.7"
  }
}
