{
  "$schema": "https://json.schemastore.org/tehuti.json",
  "model": "anthropic/claude-sonnet-4",
  "fallbackModel": "anthropic/claude-haiku-4",
  "maxTokens": 32000,
  "maxIterations": 50,
  "temperature": 0.7,
  "permissions": {
    "defaultMode": "interactive",
    "alwaysAllow": ["read", "glob", "grep", "web_fetch", "web_search"],
    "alwaysDeny": [],
    "trustedMode": false
  },
  "mcp": {
    "enabled": true,
    "servers": {}
  },
  "debug": false,
  "telemetry": false
}
